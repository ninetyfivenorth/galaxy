package_salt-openrc() {
    pkgdesc="OpenRC salt init scripts"
    depends=('openrc' 'salt')
    backup=('etc/conf.d/salt-master'
            'etc/conf.d/salt-minion'
            'etc/conf.d/salt-syncdic'
            'etc/conf.d/salt-api')

    for f in salt-master salt-minion salt-syncdic salt-api;do
        _inst_confd $f
        _inst_initd $f
    done
}

